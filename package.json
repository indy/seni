{
  "name": "seni",
  "version": "0.0.1",
  "description": "Generate Art",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "test:web": "webpack-dev-server \"mocha!./test/test.js\" --output-filename testBundle.js --config webpack.config.test.js",
    "build": "webpack --progress",
    "build:watch": "webpack --progress --watch",
    "build:min": "webpack --progress --config webpack.config.min.js",
    "clean": "rm -rf app/dist",
    "serve": "node server/server.js",
    "monitor": "nodemon server/server.js --ignore app/ --ignore test/",
    "docs": "documentation app/js/index.js -f html -o docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/indy/seni.git"
  },
  "keywords": [
    "generative art",
    "genetic algorithms"
  ],
  "author": "Inderjit Gill <email@indy.io>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/indy/seni/issues"
  },
  "homepage": "https://github.com/indy/seni",
  "dependencies": {
    "codemirror": "^5.12.0",
    "gl-matrix": "^2.3.2",
    "immutable": "^3.7.6",
    "seedrandom": "^2.4.2"
  },
  "babel": {
    "plugins": ["transform-es2015-arrow-functions",
                "transform-es2015-classes",
                "transform-es2015-object-super",
                "transform-es2015-parameters",
                "transform-es2015-destructuring",
                "transform-es2015-block-scoping",
                "transform-es2015-modules-commonjs"]
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.2",
    "babel-plugin-transform-es2015-block-scoping": "^6.5.0",
    "babel-plugin-transform-es2015-classes": "^6.5.2",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.2",
    "babel-plugin-transform-es2015-object-super": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "express": "^4.13.4",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "nodemon": "^1.9.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
