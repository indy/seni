{
  "name": "seni",
  "version": "0.0.1",
  "description": "Generate Art",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "test:web": "webpack-dev-server \"mocha!./test/test.js\" --output-filename testBundle.js --config webpack.config.test.js",
    "build": "webpack --progress",
    "build:watch": "webpack --progress --watch",
    "build:min": "webpack --progress --config webpack.config.min.js",
    "build:wasm": "wa compile -D SENI_BUILD_WASM -o app/dist/seni-wasm.wasm app/c/main_wasm.c -i app/c/seni_lang.c -i app/c/seni_parser.c -i app/c/seni_unparser.c -i app/c/seni_vm_compiler.c -i app/c/seni_vm_interpreter.c -i app/c/seni_bind.c -i app/c/seni_colour.c -i app/c/seni_parametric.c -i app/c/seni_mathutil.c -i app/c/seni_matrix.c -i app/c/seni_prng.c -i app/c/seni_render_packet.c -i app/c/seni_shapes.c -i app/c/seni_uv_mapper.c -i app/c/seni_repeat.c -i app/c/seni_path.c -i app/c/seni_focal.c -i app/c/seni_ga.c -i app/c/seni_strtof.c -i app/c/seni_printf.c -i app/c/seni_timing.c -I app/c",
    "clean": "rm -rf app/dist",
    "docs": "documentation app/js/index.js -f html -o docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/indy/seni.git"
  },
  "keywords": [
    "generative art",
    "genetic algorithms"
  ],
  "author": "Inderjit Gill <email@indy.io>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/indy/seni/issues"
  },
  "homepage": "https://github.com/indy/seni",
  "dependencies": {
    "codemirror": "^5.21.0",
    "immutable": "^3.8.1"
  },
  "babel": {
    "plugins": [
      "transform-es2015-block-scoping",       
      "transform-es2015-modules-commonjs"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-es2015-block-scoping": "^6.20.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.6.1",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.0.0",
    "webassembly": "^0.11.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
