{
  "compilerOptions": {
    "target": "es6",
    "outFile": "../www/sketch.js",
    "allowJs": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "removeComments": true
  },
  "include": [
    "./src/Log.ts",
    "./src/Matrix.ts",
    "./src/renderer.js",
    "./src/JobType.ts",
    "./src/Job.ts",
    "./src/stuff.js",
    "./src/misc.js",
    "./src/sketch.js"
  ]
}
