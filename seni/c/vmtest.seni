(rect position: [500 500]
      colour: (col/rgb g: 1)
      width: 20
      height: 20)

(fn (monkey)
  (bezier coords: [[0 -90] [50 90] [100 -90] [150 90]]
          tessellation: 30
          colour: (col/rgb b: 1))
  (rect position: [50 30]
        colour: (col/rgb r: 1)
        width: 40
        height: 40))

(on-matrix-stack
  (translate vector: [500 500])
  (repeat/rotate draw: (address-of monkey) copies: 3))
