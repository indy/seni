{
  "name": "seni",
  "version": "0.0.1",
  "description": "Generate Art",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "test:web": "webpack-dev-server \"mocha!./test/test.js\" --output-filename testBundle.js --config webpack.config.test.js",
    "build": "webpack --progress",
    "build:watch": "webpack --progress --watch",
    "build:min": "webpack --progress --config webpack.config.min.js",
    "clean": "rm -rf app/dist",
    "docs": "documentation app/js/index.js -f html -o docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/indy/seni.git"
  },
  "keywords": [
    "generative art",
    "genetic algorithms"
  ],
  "author": "Inderjit Gill <email@indy.io>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/indy/seni/issues"
  },
  "homepage": "https://github.com/indy/seni",
  "dependencies": {
    "codemirror": "^5.15.2",
    "immutable": "^3.8.1"
  },
  "babel": {
    "plugins": [
      "transform-es2015-arrow-functions",
      "transform-es2015-parameters",
      "transform-es2015-destructuring",
      "transform-es2015-block-scoping",
      "transform-es2015-modules-commonjs"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-parameters": "^6.9.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "mocha": "^2.5.2",
    "mocha-loader": "^0.7.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
